{
  "expo": {
    "name": "Who Dat? Congress Edition",
    "slug": "whodatcongress",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": "This is a flashcard app to study members of the US House of Representatives and the US Senate. It uses information from ProPublica, the @unitedstates Project, and OpenSecrets.org.",
    "githubUrl": "https://github.com/cwefso/who-dat-congress-rn"
  }
}
